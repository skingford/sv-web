{
	"name": "sv-web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:development": "vite dev --mode development",
		"dev:production": "vite dev --mode production",
		"dev:test": "vite dev --mode test",
		"build": "vite build",
		"build:development": "vite build --mode development",
		"build:production": "vite build --mode production",
		"build:test": "vite build --mode test",
		"preview": "vite preview",
		"preview:development": "vite preview --mode development",
		"preview:production": "vite preview --mode production",
		"preview:test": "vite preview --mode test",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"commitlint": "commitlint --edit",
		"commitlint:last": "commitlint --from HEAD~1 --to HEAD --verbose",
		"commitlint:ci": "commitlint --from ${{ github.event.before }} --to ${{ github.event.after }} --verbose",
		"start:dev": "./scripts/run-mode.sh dev development",
		"start:prod": "./scripts/run-mode.sh dev production",
		"start:test": "./scripts/run-mode.sh dev test"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@commitlint/types": "^19.8.1",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/postcss-pxtorem": "^6.1.0",
		"clsx": "^2.1.1",
		"husky": "^9.1.7",
		"postcss-pxtorem": "^6.1.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sass": "^1.92.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"unplugin-auto-import": "^0.18.0",
		"vite": "^7.0.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"normalize.css": "^8.0.1",
		"tw-animate-css": "^1.3.8",
		"vconsole": "^3.15.1"
	}
}
