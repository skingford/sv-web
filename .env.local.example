# 本地环境变量配置示例
# 复制此文件为 .env.local 并填入你的本地配置
# .env.local 文件会被 git 忽略，用于存储敏感信息
VITE_APP_NAME=MyLocalApp
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=A local development environment for MyLocalApp
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=5000

# API Keys (敏感信息)
VITE_API_KEY=your_api_key_here
VITE_SECRET_KEY=your_secret_key_here

# 数据库连接 (本地开发)
VITE_DB_URL=postgresql://user:password@localhost:5432/dbname

# 第三方服务 API Keys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_xxxxx
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# 开发者专用设置
VITE_DEVELOPER_MODE=true
VITE_BYPASS_AUTH=false

# 本地服务端口
VITE_LOCAL_API_PORT=3001
VITE_WEBSOCKET_PORT=3002

# 调试选项
VITE_DETAILED_LOGS=true
VITE_PERFORMANCE_MONITOR=true
